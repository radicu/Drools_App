package rules;

import com.radicu.ruleengine.model.Variable;

rule "R007"
    salience 2
when
    $fact : Variable(ncs > 500 && (sf < 1000 || bwo < 300) && xTableCurrent > 2 && yTableCurrent > 2 && spindleCurrent > 1)
then
    $fact.setState("Change Bits");
end

rule "R009"
    salience 50
when
    $fact : Variable(ph > 5000 && xTableCurrent > 2 && yTableCurrent > 2 && spindleCurrent > 1)
then
    $fact.setState("Drilling Big Hole");
end

rule "R010"
    salience 5
when
    $fact : Variable(xTableCurrent < 2 && yTableCurrent < 2)
then
    $fact.setState("Machine Inactive");
end

rule "R012"
    salience 10
when
    $fact : Variable(ph > 5000 && sf > 3000 && xTableCurrent > 2 && yTableCurrent > 2 && spindleCurrent > 1)
then
    $fact.setState("Big Drilling Check High");
end

rule "R005"
    salience 50
when
    $fact : Variable((sf > 1000 || bwo > 400) && ph < 5000 && xTableCurrent > 2 && yTableCurrent > 2 && spindleCurrent > 1)
then
    $fact.setState("Drilling Small");
end

rule "R014"
    salience 1
when
    $fact : Variable(spindleCurrent < 1 && yTableCurrent < 10 && xTableCurrent < 10 && ncs > 500)
then
    $fact.setState("Bits False Alarm Remove");
end

rule "R015"
    salience 1
when
    $fact : Variable(bwo > 1000 && sf >  6000 && spindleCurrent > 1 && ap1 < 300)
then
    $fact.setState("Bits Alarm Predict");
end

rule "R006"
    salience 50
when
    $fact : Variable((sf < 1000 || bwo < 400) || spindleCurrent < 1)
then
    $fact.setState("Spindle idle");
end

rule "R008"
    salience 50
when
    $fact : Variable((sf < 1000 || bwo < 400) && xTableCurrent < 10 && yTableCurrent < 10)
then
    $fact.setState("Warm Up");
end

rule "R011"
    salience 10
when
    $fact : Variable(ph < 5000 && sf > 6000 && xTableCurrent > 2 && yTableCurrent > 2 && spindleCurrent > 1)
then
    $fact.setState("Small Drilling Check High");
end

