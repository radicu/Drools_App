<launch4jConfig>
    <dontWrapJar>false</dontWrapJar>
    <headerType>console</headerType>

    <!-- Use relative path to jar -->
    <jar>ruleengine-0.0.1-SNAPSHOT.jar</jar>

    <!-- Use relative path to output exe -->
    <outfile>RuleEngine.exe</outfile>

    <errTitle>Java 17 Required</errTitle>
    <cmdLine/>
    <chdir>.</chdir>
    <priority>normal</priority>
    <downloadUrl/>
    <supportUrl/>
    <stayAlive>false</stayAlive>
    <restartOnCrash>false</restartOnCrash>
    <manifest/>
    <icon/>

    <jre>
        <!-- No trailing space! -->
        <path>jre17/jdk-17.0.15+6</path>
        <requiresJdk>false</requiresJdk>
        <requires64Bit>false</requires64Bit>
        <minVersion>17</minVersion>
        <maxVersion/>
    </jre>
</launch4jConfig>
